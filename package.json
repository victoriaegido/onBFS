{
    "name": "onBFS",
    "private": true,
    "version": "0.1.0-SNAPSHOT",
    "engines": {
        "node": "20.11.1"
    },
    "type": "module",
    "prettier": "@goaigua/goaigua-config/config/prettier-config/prettier.json",
    "scripts": {
        "start": "vite",
        "watch": "tsc --noEmit --watch",
        "server": "json-server --watch cypress/fixtures/db.cjs --port 4000",
        "dev": "cross-env VITE_MOCKED=true concurrently --kill-others \"pnpm run start\" \"pnpm run server\"",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "format": "prettier --write --ignore-unknown .",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "bundle-analyze": "pnpm run build && source-map-explorer 'build/static/js/*.js'",
        "cy:open": "npx cypress open",
        "cy:run": "npx cypress run",
        "vrt": "npx cypress run --spec cypress/e2e/visual-tests/*",
        "e2e": "npx cypress run --spec cypress/e2e/functional-tests/*",
        "cy:run:spec": "npx cypress run --spec",
        "prepare": "husky"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "pnpm format",
            "pnpm lint"
        ],
        "*.{css,json,html,md}": [
            "pnpm format"
        ]
    },
    "dependencies": {
        "@fontsource/open-sans": "4.5.13",
        "@goaigua/go-gisapi": "2.8.0",
        "@goaigua/goaigua-config": "0.3.1",
        "@goaigua/goaigua-styles": "1.0.0",
        "@react-keycloak/web": "3.4.0",
        "@reduxjs/toolkit": "1.9.5",
        "axios": "^1.8.0",
        "dayjs": "1.11.13",
        "devextreme": "23.1.3",
        "devextreme-react": "23.1.3",
        "eslint-config-react-app": "7.0.1",
        "highcharts": "11.1.0",
        "highcharts-react-official": "3.2.1",
        "i18next": "23.2.3",
        "immer": "10.1.1",
        "keycloak-js": "22.0.3",
        "ol": "9.2.4",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-i18next": "12.0.0",
        "react-redux": "8.1.1",
        "react-router-dom": "6.24.1"
    },
    "devDependencies": {
        "@svgr/rollup": "8.0.1",
        "@types/node": "20.3.2",
        "@types/react": "18.2.14",
        "@types/react-dom": "18.2.6",
        "@types/react-redux": "7.1.25",
        "@types/react-router-dom": "5.3.3",
        "@vitejs/plugin-react": "4.0.1",
        "concurrently": "8.2.0",
        "cross-env": "7.0.3",
        "cypress": "12.16.0",
        "cypress-image-diff-js": "1.23.0",
        "eslint": "8.43.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-cypress": "3.5.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.32.2",
        "husky": "9.1.4",
        "json-server": "0.17.3",
        "lint-staged": "15.2.9",
        "prettier": "2.8.8",
        "sass": "1.63.6",
        "source-map-explorer": "2.5.3",
        "typescript": "5.1.3",
        "vite": "4.3.9"
    }
}
